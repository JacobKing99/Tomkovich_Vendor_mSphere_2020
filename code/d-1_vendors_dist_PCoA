#Create distance matrix and generate PCoA for Schloss samples collected on day -1 of the experiment
set.dir(input=data/mothur, output=data/mothur/d-1/schloss, seed=19881117)
#set.current(processors=8)
dist.shared(shared=vendors.trim.contigs.good.unique.good.filter.unique.precluster.pick.pick.pick.opti_mcc.0.03.subsample.shared, calc=thetayc-braycurtis, subsample=5437, groups=S11Dn1E2-S12Dn1E2-S22Dn1E2-S11Dn1E1-S12Dn1E1-S21Dn1E1)
set.dir(input=data/mothur/d-1/schloss, output=data/mothur/d-1/schloss, seed=1988111)
pcoa(phylip=vendors.trim.contigs.good.unique.good.filter.unique.precluster.pick.pick.pick.opti_mcc.0.03.subsample.thetayc.0.03.lt.ave.dist)

